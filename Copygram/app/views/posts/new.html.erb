<%= form_for @post do |f| %>
	<p>
    	<%= f.label :title %>
    	<%= f.text_field :title %>
        
        <%= f.label :description %>
    	<%= f.text_field :description %>
        
    	<%= f.hidden_field :user_id %>
  	</p>
  	<p>
    	<%= f.file_field :image %>
  	</p>
 
  	<%= f.submit %>
<% end %>